<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<context-param>
		<param-name>dbHost</param-name>
		<param-value>${dbHost}</param-value>
	</context-param>
	<context-param>
		<param-name>dbPort</param-name>
		<param-value>${dbPort}</param-value>
	</context-param>
	<context-param>
		<param-name>websocketHttpPort</param-name>
		<param-value>${wsPort}</param-value>
<!-- OS		<param-value>8000</param-value> -->
<!-- LOCAL	<param-value>8080</param-value> -->
	</context-param>
	<context-param>
		<param-name>websocketHttpsPort</param-name>
		<param-value>${wssPort}</param-value>
<!-- OS		<param-value>8443</param-value> -->
<!-- LOCAL	<param-value>8080</param-value> -->
	</context-param>
</web-app>
